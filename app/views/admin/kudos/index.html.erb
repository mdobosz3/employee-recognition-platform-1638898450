<p id="notice"><%= notice %></p>

<h1>Admin Kudos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_kudos.each do |admin_kudo| %>
      <tr>
        <td><%= link_to 'Show', admin_kudo %></td>
        <td><%= link_to 'Edit', edit_admin_kudo_path(admin_kudo) %></td>
        <td><%= link_to 'Destroy', admin_kudo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Kudo', new_admin_kudo_path %>
